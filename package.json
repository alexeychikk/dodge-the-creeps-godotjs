{
  "name": "rogalik",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "godot-ts build",
    "dev": "npm-run-all build generate -p watch watch-generate open-editor",
    "generate": "godot-ts generate",
    "open-editor": "godot -e --path .",
    "start": "npm run build && godot",
    "watch": "godot-ts watch",
    "watch-generate": " nodemon --exec \"godot-ts generate\" -e gd",
    "clean": "rimraf dist .godot generated",
    "lint": "run-p -s lint:tsc lint:eslint lint:prettier",
    "lint:fix": "run-s lint:eslint:fix lint:prettier:fix",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write ."
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@godot-js/godot-ts": "^0.0.8",
    "@types/node": "^22.7.3",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "globals": "^16.3.0",
    "nodemon": "3.1.10",
    "npm-run-all2": "8.0.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.41.0"
  },
  "godot-ts": {
    "src": ".",
    "out": "./.godot/GodotJS",
    "minifyClasses": true
  }
}
